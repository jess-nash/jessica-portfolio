<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1">
    <!-- Use the title from a page's frontmatter if it has one -->
    <%# <title><%= current_page.data.title || "Jessica Nash | Web Development" %></title>
    <title>Jessica Nash | Web Development</title>

      <!-- Primary Meta Tags -->
    <meta name="title" content="Jessica Nash | Fullstack Developer">
    <meta name="description" content="Clean code, problem solving, fresh ideas. That's me!">
    <meta property="og:image" content=”../images/meta-image.png”>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v6.1.2/css/all.css">
    <link rel="apple-touch-icon" sizes="180x180" href="../images/favicon_io/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="../images/favicon_io/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="../images/favicon_io/favicon-16x16.png">

    <%= stylesheet_link_tag "site" %>
    <%= javascript_include_tag "site" %>
    <%= display_meta_tags %>
  </head>
  <body>
    <!-- NavBar Section -->
    <%= partial 'partials/navbar' %>

    <%# <div class="alert alert-info" role="alert">
      <h4 class="alert-heading">Sorry!</h4>
      <p>This site on mobile devices is currently under construction.</p>
      <hr>
      <p class="mb-0">Try visiting me on a desktop! Or come back again soon.</p>
      <p class="mb-0">Sorry for the inconvenience.</p>
    </div> %>

    <!-- Social buttons Section -->
    <%= partial 'partials/social-buttons' %>

    <%= yield %>
  </body>
</html>
